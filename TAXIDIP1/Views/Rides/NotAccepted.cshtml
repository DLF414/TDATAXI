@model IEnumerable<TAXIDIP1.Models.Rides>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NotAccepted</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AddressStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddressEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AddressStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddressEnd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                <a asp-action="Accept" asp-route-id="@item.Id">Принять</a> 
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
